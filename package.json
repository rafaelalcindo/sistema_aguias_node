{
  "name": "sistema_aguias",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento do Clube Aguias da Colina",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev:server": "ts-node-dev --inspect --transpile-only --ignore node_modules src/server.ts",
    "start": "node dist/server.js",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeormNode": "node ./node_modules/typeorm/cli.js",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/database/index.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/database/index.ts"
  },
  "author": "Rafael Alcindo de LIma",
  "license": "ISC",
  "dependencies": {
    "@join-com/typeorm-class-validator-is-uniq": "^2.0.0",
    "archiver": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "excel4node": "^1.8.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "puppeteer": "^19.2.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "0.2.41",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.188",
    "@types/md5": "^2.3.2",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/swagger-ui-express": "^4.1.3",
    "do-devops": "^0.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
